# │   ├── exporters.py       
# │   ├── filters.py         
# │   ├── adapters.py        
# │   └── utils.py
# ├── tests/
# │   ├── __init__.py
# │   ├── test_core.py
# │   ├── test_formatters.py
# │   ├── test_exporters.py
# │   └── test_handlers.py
# ├── examples/
# │   ├── basic_usage.py
# │   ├── advanced_usage.py
# │   ├── json_logging.py
# │   ├── elasticsearch_example.py 
